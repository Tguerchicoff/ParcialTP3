        android:label="fragment_welcome_screen"
        tools:layout="@layout/fragment_welcome_screen" >
        <action
            android:id="@+id/action_welcomeScreenFragment_to_fakeLoginFragment"
            app:destination="@id/fakeLoginFragment" />
    </fragment>
    <fragment
        android:id="@+id/fakeLoginFragment"
        android:name="com.ort.edu.ar.parcialtp3.Fragments.FakeLoginFragment"
